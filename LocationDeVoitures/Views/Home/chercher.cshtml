@model IEnumerable<LocationDeVoitures.Models.Advertisement>
@{
    ViewData["Title"] = "chercher";
}
<h1>chercher+@ViewBag.nom</h1>
<head>
    <style type="text/css">
        .content {
            width: 800px;
            margin-left: 100px;
            margin-right: 100px;
        }
    </style>
</head>
<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                <div class="col-sm-6">
                    <ol class="breadcrumb float-sm-right">
                        <li class="breadcrumb-item"><a href="#">Home</a></li>
                        <li class="breadcrumb-item active">Blank Page</li>
                    </ol>
                </div>
            </div>
        </div><!-- /.container-fluid -->
    </section>

    <!-- Main content -->
    <section class="content">


        <!-- Default box -->
        @foreach (var item in Model)
        {
            @await Html.PartialAsync("DisplayAnnonce", item)

        }

        <!-- /.card -->

    </section>
    <!-- /.content -->
</div>


